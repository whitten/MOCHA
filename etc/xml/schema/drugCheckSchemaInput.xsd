<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="ext/domain/pharmacy/peps/external/common/preencapsulation/vo/drug/check/request"
    targetNamespace="ext/domain/pharmacy/peps/external/common/preencapsulation/vo/drug/check/request"
    elementFormDefault="qualified"
    attributeFormDefault="unqualified">
    <xsd:element name="PEPSRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="Header" />
                <xsd:element
                    ref="Body"
                    minOccurs="0" />
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Header">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="Time" />
                <xsd:element ref="MServer" />
                <xsd:element ref="MUser" />
            </xsd:sequence>
            <xsd:attribute
                name="pingOnly"
                type="xsd:boolean"
                use="optional"
                default="false" />
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Time">
        <xsd:complexType>
            <xsd:attribute
                name="value"
                type="xsd:string"
                use="required" />
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="MServer">
        <xsd:complexType>
            <xsd:attribute
                name="stationNumber"
                type="xsd:unsignedLong"
                use="required" />
            <xsd:attribute
                name="serverName"
                type="xsd:string"
                use="required" />
            <xsd:attribute
                name="ip"
                type="xsd:string"
                use="required" />
            <xsd:attribute
                name="uci"
                type="xsd:string"
                use="required" />
            <xsd:attribute
                name="namespace"
                type="xsd:string"
                use="required" />
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="MUser">
        <xsd:complexType>
            <xsd:attribute
                name="jobNumber"
                type="xsd:unsignedLong"
                use="required" />
            <xsd:attribute
                name="duz"
                type="xsd:unsignedLong"
                use="required" />
            <xsd:attribute
                name="userName"
                type="xsd:string"
                use="required" />
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Body">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="drugCheck" />
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="drugCheck">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="checks" />
                <xsd:element
                    ref="prospectiveDrugs"
                    minOccurs="0" />
                <xsd:element
                    ref="medicationProfile"
                    minOccurs="0" />
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="checks">
        <xsd:complexType>
            <xsd:all>
                <xsd:element
                    ref="drugDrugCheck"
                    minOccurs="0" />
                <xsd:element
                    ref="drugTherapyCheck"
                    minOccurs="0" />
                <xsd:element
                    ref="drugDoseCheck"
                    minOccurs="0" />
            </xsd:all>
            <xsd:attribute
                name="prospectiveOnly"
                type="xsd:boolean"
                use="required" />
            <xsd:attribute
                name="useCustomTables"
                type="xsd:boolean"
                use="optional"
                default="true" />
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="drugDrugCheck">
        <xsd:complexType />
    </xsd:element>
    <xsd:element name="drugTherapyCheck">
        <xsd:complexType>
            <xsd:attribute
                name="useDuplicateAllowance"
                type="xsd:boolean"
                use="optional"
                default="true" />
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="drugDoseCheck">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="demographics" />
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="demographics">
        <xsd:complexType>
            <xsd:attribute
                name="ageInDays"
                use="required">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:long">
                        <xsd:minExclusive value="0" />
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute
                name="weightInKG"
                use="required">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:double">
                        <xsd:minInclusive value="0" />
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute
                name="bodySurfaceAreaInSqM"
                use="required">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:double">
                        <xsd:minInclusive value="0" />
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="medicationProfile">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element
                    ref="drug"
                    minOccurs="0"
                    maxOccurs="unbounded" />
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="prospectiveDrugs">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element
                    ref="drug"
                    maxOccurs="unbounded" />
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="drug">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element
                    ref="doseInformation"
                    minOccurs="0" />
            </xsd:sequence>
            <xsd:attribute
                name="gcnSeqNo"
                type="xsd:integer"
                use="required" />
            <xsd:attribute
                name="vuid"
                use="optional">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:integer">
                        <xsd:minInclusive value="0" />
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute
                name="drugName"
                type="xsd:string"
                use="optional" />
            <xsd:attribute
                name="ien"
                use="optional">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:integer">
                        <xsd:minInclusive value="0" />
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute
                name="orderNumber"
                type="xsd:string"
                use="required" />
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="doseInformation">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element
                    name="doseType"
                    type="doseTypeType" />
                <xsd:element name="doseAmount">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:double">
                            <xsd:minExclusive value="0" />
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element
                    name="doseUnit"
                    type="doseUnitType" />
                <xsd:element
                    name="doseRate"
                    type="doseRateType" />
                <xsd:element
                    name="frequency"
                    type="xsd:string" />
                <xsd:element name="duration">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:integer">
                            <xsd:minExclusive value="0" />
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element
                    name="durationRate"
                    type="durationRateType" />
                <xsd:element
                    name="route"
                    type="routeType" />
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
